package com;

public class Car {
	int wheel;
	float price;
	String colour;
	
	Car() {
		System.out.println("object created...");
		wheel = 4;
		price = 1000000;
		colour = "Don't known";
	}
	
	void start() {
		System.out.println("Car Start");
	}
	void appliedGear() {
		
	}
	void moving() {
		
	}
	void stop() {
		System.out.println("Car stop");
	}
	void displayCarDetails(String info) {
		String msg="Car Details : "+info;
		System.out.println(msg);
		System.out.println("Wheel "+wheel);
		System.out.println("colour "+colour);
		System.out.println("price "+price);
	}
}
